{
  "$schema": "http://json-schema.org/schema",
  "id": "AngularTSLintSchematics",
  "title": "angular tslint schematics",
  "type": "object",
  "properties": {
    "prefix": {
      "type": "string",
      "description": "component and directive prefix",
      "default": "app",
      "x-prompt": {
        "message": "What prefix would you like to use for the component and directive?",
        "type": "input"
      }
    }
  }
}
